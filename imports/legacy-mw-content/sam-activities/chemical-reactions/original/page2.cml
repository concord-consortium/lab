<component><class>org.concord.modeler.PageTextBox</class>
<width>250.0</width>
<height>275.0</height>
<title>&lt;html&gt;
  &lt;head&gt;
    
  &lt;/head&gt;
  &lt;body marginwidth=&quot;10&quot; marginheight=&quot;10&quot;&gt;
    &lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; width=&quot;100%&quot; cellpadding=&quot;0&quot;&gt;
      &lt;tr&gt;
        &lt;td&gt;
          &lt;font size=&quot;4&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;&lt;strong&gt;Follow 
          these steps:&lt;/strong&gt;&lt;/font&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;
          &lt;font size=&quot;4&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;1. Add 
          some atoms (press multiple times to add more):&lt;/font&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td align=&quot;center&quot;&gt;
          &lt;input script=&quot;
		script:mw:1:
		define %anum %number_of_atoms+2; 
		if (%anum &lt;= 50); 
		   while (%number_of_atoms &lt; %anum); 
		      add atom 1 (rand()*%width, rand()*%height); 
		   endwhile; 
		else;
		   message &lt;t&gt;Maximum Number of Atoms Reached&lt;/t&gt;;
		endif;&quot; value=&quot;add 2&quot; type=&quot;submit&quot; name=&quot;Submit&quot;&gt;
          &amp;#160; &lt;input script=&quot;
		script:mw:1:define %anum %number_of_atoms+10; 
		if (%anum &lt;= 50); 
		   while (%number_of_atoms &lt; %anum); 
		      add atom 1 (rand()*%width, rand()*%height); 
		   endwhile; 
		else;
		   message &lt;t&gt;Maximum Number of Atoms Reached&lt;/t&gt;;
		endif;&quot; value=&quot;add 10&quot; type=&quot;submit&quot; name=&quot;Submit2&quot;&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;
          &lt;font size=&quot;4&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;&lt;br&gt;
          2. Run &lt;img height=&quot;12&quot; src=&quot;play.gif&quot; width=&quot;7&quot;&gt; the model. &lt;/font&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;
          &lt;font size=&quot;4&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;&lt;br&gt;
          3. Reset &lt;img height=&quot;12&quot; src=&quot;reset.gif&quot; width=&quot;9&quot;&gt; the model and 
          try a different concentration.&lt;/font&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
        &lt;td&gt;
          &lt;font size=&quot;3&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;&lt;br&gt;
          &lt;/font&gt;&lt;font color=&quot;#990000&quot; size=&quot;3&quot; face=&quot;Verdana, Arial, Helvetica, sans-serif&quot;&gt;Gauge 
          the reaction speed by using the graph to see when the reaction 
          reaches 80% completion. &lt;/font&gt;
        &lt;/td&gt;
      &lt;/tr&gt;
    &lt;/table&gt;
  &lt;/body&gt;
&lt;/html&gt;
</title>
