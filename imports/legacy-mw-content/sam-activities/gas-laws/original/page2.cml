<component><class>org.concord.modeler.PageButton</class>
<title>Remove an atom from the balloon</title>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<disabled_at_script>true</disabled_at_script>
<continuous_fire>true</continuous_fire>
<script>define %i 0;
define %count 0;
while(%number_of_atoms &gt; %i);
  if(%atom[%i].id == %leftid);
    set %count %count+1;
  endif;
  set %i %i+1;
endwhile;
set %count %count-1;
set %i 0;
while(%number_of_atoms &gt; %i);
  if(%atom[%i].id == %leftid);
    select atom %i;
    remove;
    set textbox[3].text &lt;t&gt;formatvar(%3.0f, %count)&lt;/t&gt;;
    return;
  endif;
  set %i %i+1;
endwhile;
</script>
<action>Execute MW script</action>
</component></content>

<content start="19" end="20">
</content>

<content start="20" end="21">
<component><class>org.concord.modeler.PageButton</class>
<title>Add an atom to the balloon</title>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<disabled_at_script>true</disabled_at_script>
<continuous_fire>true</continuous_fire>
<script>define %x rand()*(%obstacle[0].x-%element[%leftid].sigma);
define %y rand()*%height;
define %overlap 0;
define %i 0;
define %dx 0;
define %dy 0;
define %rsq %element[%leftid].sigma*%element[%leftid].sigma;
define %count 0;

// detect overlap
while (%number_of_atoms &gt; %i);
  if(%atom[%i].id == %leftid) {
    set %dx %x - %atom[%i].rx;
    set %dy %y - %atom[%i].ry;
    if(%dx*%dx + %dy*%dy &lt; %rsq);
      set %overlap 1;
    endif;
  endif;
  set %i %i+1;
endwhile;

// if no overlap, add atom
if(%overlap == 0);
  add atom %leftid (%x, %y);
endif;

// count atoms
set %i 0;
while(%number_of_atoms &gt; %i);
  if(%atom[%i].id == %leftid);
    set %count %count+1;
  endif;
  set %i %i+1;
endwhile;
set textbox[3].text &lt;t&gt;formatvar(%3.0f,%count)&lt;/t&gt;;
</script>
<action>Execute MW script</action>
</component></content>

