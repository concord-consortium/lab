     <void property="script"> 
      <string>set %flag count(-1, %rectangle[2].x,  %rectangle[2].y,  %rectangle[2].width,  %rectangle[2].height);
if (%flag &gt; 0);
	stop;
	message youWin.html;
else;
	set %i 0;
	while(%i &lt; %number_of_obstacles);
		set %flag count(-1, %obstacle[%i].x-%d, %obstacle[%i].y-%d, %obstacle[%i].width + 2*%d, %obstacle[%i].height + 2*%d);
		if(%flag &gt; 0);
			stop;
			message collision.html;
			select element 1;
			remove;
			set %flag 0;
		endif;
		%i++;
	endwhile;
endif;</string> 
