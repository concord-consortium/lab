  <void property="script"> 
   <string>define %move 1;
define %addPos 2;
define %addNeg 3;
define %addElectron 4;
define %buttonSelected %move;
define %selAtom -1;

beginmouse:pressed;
	set %selAtom whichParticle(%mouse_x/10,%mouse_y/10);
	if (%selAtom == -1);
		select atom none;
	endif;
endmouse:pressed;

beginmouse:released;
	if (%buttonSelected == %addPos);
		set charge 1;
		set movable off;
		select atom none;
	endif;
	if (%buttonSelected == %addNeg);
		set charge -1;
		set movable off;
		select atom none;
	endif;
	if (%buttonSelected == %addElectron);
		select element 3;
		remove;
		add atom 3 (%mouse_x/10,%mouse_y/10);
		set %eIndex %number_of_atoms-1;
		if (%atom[%eIndex].id == 3);
			set atom[%eIndex].charge -1;
			set atom[%eIndex].custom -1000;
            select atom %eIndex;
            traj on;
			select atom none;
			run;
		endif;
	endif;
	paint;
endmouse:released;</string> 
  </void> 
