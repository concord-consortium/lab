  <void property="script"> 
   <string>define %i 0;
define %k 0;
define %count 0;
define %d 0.6*%atom[0].sigma;
define %dSq %d*%d;
define %x 0;
define %y 0;
define %w 0;
define %h 0;
define %x1 0;
define %y1 0;
define %x2 0;
define %y2 0;
define %x3 0;
define %y3 0;
define %distSq1;
define %distSq2;
define %distSq3;
define %px 0;
define %py 0;
define %flag 0;
define %oldFlag1 array(%number_of_rectangles);
define %oldFlag2 array(%number_of_triangles);</string> 
  </void> 
  <void property="tasks"> 
   <void method="add"> 
    <object class="org.concord.mw2d.models.TaskAttributes"> 
     <void property="description"> 
      <string>Count the collisions between the particle and the rectangles</string> 
     </void> 
     <void property="name"> 
      <string>Collision detection for rectangles</string> 
     </void> 
     <void property="script"> 
      <string>set %i 0;
set %wt within(RECTANGLE, %atom[0].rx, %atom[0].ry, 10);
while(%i &lt; %wt.length);
  set %k %wt[%i];
  set %x %rectangle[%k].x - %d;
  set %y %rectangle[%k].y - %d;
  set %w %rectangle[%k].width + 2 * %d;
  set %h %rectangle[%k].height + 2 * %d;
  set %flag count(-1, %x, %y, %w, %h);
  if(%flag &gt; 0);
    if(%oldFlag1[%k] &lt; 0.5);
      %count++;
      sound click.wav;
      set %oldFlag1[%k] 1;
    endif;
  else;
    set %oldFlag1[%k] 0;
  endif;
  %i++;
endwhile;
</string> 
     </void> 
    </object> 
   </void> 
   <void method="add"> 
    <object class="org.concord.mw2d.models.TaskAttributes"> 
     <void property="description"> 
      <string>Count the collisions between the particle and the triangles</string> 
     </void> 
     <void property="name"> 
      <string>Collision detection for triangles</string> 
     </void> 
     <void property="script"> 
      <string>set %i 0;
set %wt within(TRIANGLE, %atom[0].rx, %atom[0].ry, 10);
set %px %atom[0].rx;
set %py %atom[0].ry;
while(%i &lt; %wt.length);
  set %k %wt[%i];
  set %x1 %triangle[%k].x1;
  set %y1 %triangle[%k].y1;
  set %x2 %triangle[%k].x2;
  set %y2 %triangle[%k].y2;
  set %x3 %triangle[%k].x3;
  set %y3 %triangle[%k].y3;
  set %distSq1 ptSegDistSq(%x1, %y1, %x2, %y2, %px, %py);
  set %distSq2 ptSegDistSq(%x2, %y2, %x3, %y3, %px, %py);
  set %distSq3 ptSegDistSq(%x3, %y3, %x1, %y1, %px, %py);
  if(%distSq1 &lt; %dSq or %distSq2 &lt; %dSq or %distSq3 &lt; %dSq );
    if(%oldFlag2[%k] &lt; 0.5);
      %count++;
      sound click.wav;
      set %oldFlag2[%k] 1;
    endif;
  else;
    set %oldFlag2[%k] 0;
  endif;
  %i++;
endwhile;
</string> 
     </void> 
    </object> 
   </void> 
