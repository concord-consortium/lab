<component><class>org.concord.modeler.PageApplet</class>
<appletjar>lapack.jar</appletjar>
<appletjar>qm.jar</appletjar>
<appletclass>org.concord.qm1d.MainWindow</appletclass>
<uid>qm1d</uid>
<resource>jmolapplet$applet$0.aps</resource><parameter>{name=&quot;potential&quot; value=&quot;CRYSTAL_LATTICE&quot;}
