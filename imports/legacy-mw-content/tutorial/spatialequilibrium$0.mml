     <void property="script"> 
      <string>define %cx 0;
define %cy 0;
define %i 0;
if(50 &gt; %model_time);
  set %count 0; // reset the counter when the model is reset;
endif;
while(%number_of_atoms &gt; %i);
  set %cx %cx+%atom[%i].rx;
  set %cy %cy+%atom[%i].ry;
  set %i %i+1;
endwhile;
set %cx %cx/%number_of_atoms;
set %cy %cy/%number_of_atoms;
set %cx %cx-%width/2;
set %cy %cy-%height/2;
if(9 &gt; %cx*%cx + %cy*%cy);
  print %count, %model_time, (%cx, %cy);
  if(%count &gt; 5); // let it equilibrate for 5 times
    stop immediately;
    message &lt;t&gt;Spatial equilibrium is reached!&lt;/t&gt;
    delay 2;
    snapshot;
  endif;
  set %count %count+1;
endif;</string> 
