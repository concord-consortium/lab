<title>&lt;html&gt;
  &lt;head&gt;
    
  &lt;/head&gt;
  &lt;body&gt;
    &lt;pre&gt;&lt;font size=&quot;3&quot;&gt;stop;
define %x1 %width/4;
define %y1 %height/4;
define %x2 3*%width/4;
define %y2 3*%height/4;
define %in0 %atom[0].rx &amp;gt; %x1 and %atom[0].ry &amp;gt; %y1 and %x2 &amp;gt; %atom[0].rx and %y2 &amp;gt; %atom[0].ry;
define %in1 %atom[1].rx &amp;gt; %x1 and %atom[1].ry &amp;gt; %y1 and %x2 &amp;gt; %atom[1].rx and %y2 &amp;gt; %atom[1].ry;
if(%in0);
  if(%in1);
    message &lt;t&gt;&lt;/font&gt;&lt;b&gt;&lt;font color=&quot;gray&quot; size=&quot;3&quot;&gt;Yes!&lt;/font&gt;&lt;/b&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
  else;
    message &lt;t&gt;&lt;/font&gt;&lt;font color=&quot;red&quot; size=&quot;3&quot;&gt;Atom 1 is not inside.&lt;/font&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
  endif;
else;
  if(%in1);
    message &lt;t&gt;&lt;/font&gt;&lt;font color=&quot;red&quot; size=&quot;3&quot;&gt;Atom 0 is not inside.&lt;/font&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
  else;
    message &lt;t&gt;&lt;/font&gt;&lt;font color=&quot;red&quot; size=&quot;3&quot;&gt;Neither is inside.&lt;/font&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
  endif;
endif;
define %inAnd (%in0 and %in1);
print %in0 and %in1 = %inAnd;
define %inOr (%in0 or %in1);
print %in0 or %in1 = %inOr;&lt;/font&gt;&lt;/pre&gt;
  &lt;/body&gt;
&lt;/html&gt;
</title>
