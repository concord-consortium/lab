define %dy %atom[0].ry-%atom[1].ry;
if(100&amp;gt;(%dx*%dx+%dy*%dy));
  print yes;
  message &lt;t&gt;&lt;/font&gt;&lt;b&gt;&lt;font color=&quot;gray&quot; size=&quot;2&quot;&gt;Yes!&lt;/font&gt;&lt;/b&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
else;
  print no;
  message &lt;t&gt;&lt;/font&gt;&lt;font color=&quot;red&quot; size=&quot;3&quot;&gt;No!&lt;/font&gt;&lt;font size=&quot;3&quot;&gt;&lt;/t&gt;;
endif;&lt;/font&gt;&lt;/pre&gt;
  &lt;/body&gt;
&lt;/html&gt;
</title>
<opaque>false</opaque>
