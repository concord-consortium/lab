add atom 2 (18.0 12.0); delay 0.1;
delay 0.2;
add atom 1 (10.0 14.0); delay 0.1;
add atom 2 (12.0 14.0); delay 0.1;
add atom 1 (14.0 14.0); delay 0.1;
add atom 2 (16.0 14.0); delay 0.1;
add atom 1 (18.0 14.0); delay 0.1;
delay 0.2;
add atom 2 (10.0 16.0); delay 0.1;
add atom 1 (12.0 16.0); delay 0.1;
add atom 2 (14.0 16.0); delay 0.1;
add atom 1 (16.0 16.0); delay 0.1;
add atom 2 (18.0 16.0); delay 0.1;
delay 0.2;
add atom 1 (10.0 18.0); delay 0.1;
add atom 2 (12.0 18.0); delay 0.1;
