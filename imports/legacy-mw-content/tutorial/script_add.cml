<border>Raised Bevel</border>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<script>select atom all;
remove;
delay 0.2;
add atom 1 (10.0 10.0); delay 0.1;
add atom 2 (12.0 10.0); delay 0.1;
add atom 1 (14.0 10.0); delay 0.1;
add atom 2 (16.0 10.0); delay 0.1;
add atom 1 (18.0 10.0); delay 0.1;
delay 0.2;
add atom 2 (10.0 12.0); delay 0.1;
