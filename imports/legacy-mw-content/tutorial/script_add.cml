<paragraph start="5291" end="5297">
<Alignment>1</Alignment>
<LeftIndent>140.0</LeftIndent>
<content start="5291" end="5292">
<component><class>org.concord.modeler.PageButton</class>
<title>Make a square lattice</title>
<tooltip>Construct a square lattice</tooltip>
<border>Raised Bevel</border>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<script>select atom all;
remove;
select image all;
remove;
delay 0.2;
define %i 0;
while(%i&lt;5);
  if(mod(%i,2)==0);
    add atom 1 (10 + %i * 2, 10);
  else;
    add atom 2 (10 + %i * 2, 10);
  endif;
  delay 0.1;
  set %i %i+1;
endwhile;
delay 0.2;

set %i 0;
while(%i&lt;5);
  if(mod(%i, 2)==0);
    add atom 2 (10 + %i * 2, 12);
  else;
    add atom 1 (10 + %i * 2, 12);
  endif;
  delay 0.1;
  set %i %i+1;
endwhile;
delay 0.2;

set %i 0;
while(%i&lt;5);
  if(mod(%i,2)==0);
    add atom 1 (10 + %i * 2, 14);
  else;
    add atom 2 (10 + %i * 2, 14);
  endif;
  delay 0.1;
  set %i %i+1;
endwhile;
delay 0.2;

set %i 0;
while(%i&lt;5);
  if(mod(%i,2)==0);
    add atom 2 (10 + %i * 2, 16);
  else;
    add atom 1 (10 + %i * 2, 16);
  endif;
  delay 0.1;
  set %i %i+1;
endwhile;
delay 0.2;

set %i 0;
while(%i&lt;5);
  if(mod(%i,2)==0);
    add atom 1 (10+%i*2, 18);
  else;
    add atom 2 (10 + %i * 2, 18);
  endif;
  delay 0.1;
  set %i %i + 1;
endwhile;</script>
<action>Execute MW script</action>
</component></content>

