      &nbsp;&nbsp;
      %a{:href => 'https://groups.google.com/group/lab-models'} Developer Mail List
      &nbsp;&nbsp;
      %a{:href => '../../experiments/navigator/index.html'} Browser Check

    .column1
      .item-list
        %h2
          Lab Model Types
        %ul
          %li
            %a{:href => 'md2d/index.html'} Next Generation Molecular Workbench
          %li
            %a{:href => 'solar-system/index.html'} Solar System
