{
  "title": "Rectangles",
  "publicationStatus": "public",
  "subtitle": "",
  "about": "",
  "models": [
    {
      "type": "md2d",
      "id": "rectangle$0",
      "url": "imports/legacy-mw-content/converted/conversion-and-physics-examples/rectangle$0.json",
      "viewOptions": {
        "controlButtons": "play_reset_step",
        "modelTimeLabel": true
      }
    }
  ],
  "outputs": [
    {
      "name": "atomswithin",
      "unitType": "",
      "label": "Atoms within",
      "value": [
        "return getAtomsWithinShape(1).length;"
      ]
    },
    {
      "name": "kewithin",
      "unitType": "temperature",
      "label": "Temperature within",
      "value": [
        "var within=getAtomsWithinShape(1);",
        "return atomsKe(within)/within.length;"
      ]
    },
    {
      "name": "ketotal",
      "unitType": "temperature",
      "label": "Temperature overall",
      "value": [
        "return ke()/getNumberOfAtoms();"
      ]
    }
  ],
  "components": [
    {
      "type": "slider",
      "id": "xslider",
      "min": 0,
      "max": 2.8,
      "initialValue": 0.34,
      "displayValue": "return format('.3f')(value)",
      "labels": [
        {
          "value": 0,
          "label": "Left"
        },
        {
          "value": 2.8,
          "label": "Right"
        }
      ],
      "steps": 199,
      "title": "Rectangle Distance to left",
      "action": "setShapeProperties(1, { x: value });"
    },
    {
      "type": "slider",
      "id": "wslider",
      "min": 0,
      "max": 2.8,
      "initialValue": 0.76,
      "displayValue": "return format('.3f')(value)",
      "labels": [
        {
          "value": 0,
          "label": "Thin"
        },
        {
          "value": 2.8,
          "label": "Wide"
        }
      ],
      "steps": 199,
      "title": "Rectangle Width",
      "action": "setShapeProperties(1, { width: value });"
    },
    {
      "type": "slider",
      "id": "yslider",
      "min": 0,
      "max": 2,
      "initialValue": 0.45,
      "displayValue": "return format('.3f')(value)",
      "labels": [
        {
          "value": 0,
          "label": "Bottom"
        },
        {
          "value": 2,
          "label": "Top"
        }
      ],
      "steps": 199,
      "title": "Rectangle Distance to bottom",
      "action": "setShapeProperties(1, { y: value });"
    },
    {
      "type": "slider",
      "id": "hslider",
      "min": 0,
      "max": 2,
      "initialValue": 1.82,
      "displayValue": "return format('.3f')(value)",
      "labels": [
        {
          "value": 0,
          "label": "Short"
        },
        {
          "value": 2,
          "label": "Tall"
        }
      ],
      "steps": 199,
      "title": "Rectangle Height",
      "action": "setShapeProperties(1, { height: value });"
    },
    {
      "type": "numericOutput",
      "id": "atoms-within",
      "title": "Atoms in rectangle",
      "property": "atomswithin",
      "units": false,
      "labelOn":"left"
    },
    {
      "type": "checkbox",
      "id": "isfence",
      "initialValue":true,
      "text": "Yellow rectangle is fence",
      "onClick": "setShapeProperties(4,{fence:value});"
    },
    {
      "type": "barGraph",
      "id": "average-ke",
      "title": "Average KE in rectangle",
      "property": "kewithin",
      "units": true,
      "labelFormat": "1r",
      "min": 0,
      "max": 0.05
    },
    {
      "type": "barGraph",
      "id": "total-ke",
      "title": "Average KE of all atoms",
      "property": "ketotal",
      "units": true,
      "labelFormat": "1r",
      "min": 0,
      "max": 0.05
    }
  ],
  "layout": {
    "right": [
      "average-ke",
      "total-ke"
    ],
    "bottom": [
      [
        "xslider",
